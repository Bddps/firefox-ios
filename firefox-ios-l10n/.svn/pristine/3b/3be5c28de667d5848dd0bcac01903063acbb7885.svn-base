      <trans-unit id="Authentication required" approved="yes">
        <source>Authentication required</source>
        <target state="translated">Vyžadována autentizace</target> <note>Authentication prompt title</note>
      </trans-unit>
      <trans-unit id="Back" approved="yes">
        <source>Back</source>
        <target state="translated">Zpět</target> <note>Accessibility Label for the browser toolbar Back button</note>
      </trans-unit>
      <trans-unit id="Block Pop-up Windows" approved="yes">
        <source>Block Pop-up Windows</source>
        <target state="translated">Blokovat vyskakovací okna</target> <note>Block pop-up windows setting</note>
      </trans-unit>
      <trans-unit id="Bookmark" approved="yes">
        <source>Bookmark</source>
        <target state="translated">Přidat do záložek</target> <note>Bookmark a URL - https://bugzilla.mozilla.org/attachment.cgi?id=8624438, https://bug1157303.bugzilla.mozilla.org/attachment.cgi?id=8624440</note>
      </trans-unit>
      <trans-unit id="Bookmarks" approved="yes">
        <source>Bookmarks</source>
        <target state="translated">Záložky</target> <note>Panel accessibility label</note>
      </trans-unit>
      <trans-unit id="Brightness" approved="yes">
        <source>Brightness</source>
        <target state="translated">Jas</target> <note>Accessibility label for brightness adjustment slider in Reader Mode display settings</note>
      </trans-unit>
      <trans-unit id="Call" approved="yes">
        <source>Call</source>
        <target state="translated">Volat</target> <note>Alert Call Button</note>
      </trans-unit>
      <trans-unit id="Cancel" approved="yes">
        <source>Cancel</source>
        <target state="translated">Zrušit</target> <note>Cancel button title in share dialog</note>
      </trans-unit>
      <trans-unit id="Changes color theme." approved="yes">
        <source>Changes color theme.</source>
        <target state="translated">Změní barevné schéma.</target> <note>Accessibility hint for the color theme setting buttons in reader mode display settings</note>
      </trans-unit>
      <trans-unit id="Changes font type." approved="yes">
        <source>Changes font type.</source>
        <target state="translated">Změní typ písma.</target> <note>Accessibility hint for the font type buttons in reader mode display settings</note>
      </trans-unit>
      <trans-unit id="Clear Private Data" approved="yes">
        <source>Clear Private Data</source>
        <target state="translated">Vymazat soukromá data</target> <note>Label used as an item in Settings. When touched it will open a dialog prompting the user to make sure they want to clear all of their private data.</note>
      </trans-unit>
      <trans-unit id="Close" approved="yes">
        <source>Close</source>
        <target state="translated">Zavřít</target> <note>Accessibility label for action denoting closing a tab in tab list (tray)</note>
