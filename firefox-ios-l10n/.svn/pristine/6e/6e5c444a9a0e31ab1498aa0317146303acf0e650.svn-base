  <file original="Client/Intro.strings" source-language="en" datatype="plaintext" target-language="sl">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="7.0" build-num="7A220"/>
    </header>
    <body>
      <trans-unit id="Browse multiple Web pages at the same time with tabs." approved="yes">
        <source>Browse multiple Web pages at the same time with tabs.</source>
        <target state="translated">Brskajte po več spletnih straneh hkrati s pomočjo zavihkov.</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Choose" approved="yes">
        <source>Choose</source>
        <target state="translated">Izberi</target> <note>See http://mzl.la/1if9ODp</note>
      </trans-unit>
      <trans-unit id="Connect Firefox everywhere you use it." approved="yes">
        <source>Connect Firefox everywhere you use it.</source>
        <target state="translated">Povežite Firefox, kjerkoli ga uporabljate.</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Customize" approved="yes">
        <source>Customize</source>
        <target state="translated">Prilagodi</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Easily switch between open pages with tabs." approved="yes">
        <source>Easily switch between open pages with tabs.</source>
        <target state="translated">Preklapljajte odprte strani s pomočjo zavihkov.</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Firefox and the cloud" approved="yes">
        <source>Firefox and the cloud</source>
        <target state="translated">Firefox in oblak</target> <note>Accessibility label for an image. See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Introductory slide %@ of %@" approved="yes">
        <source>Introductory slide %1$@ of %2$@</source>
        <target state="translated">Predstavitvena stran %1$@ od %2$@</target> <note>String spoken by assistive technology (like VoiceOver) stating on which page of the intro wizard we currently are. E.g. Introductory slide 1 of 3</note>
      </trans-unit>
      <trans-unit id="Organize" approved="yes">
        <source>Organize</source>
        <target state="translated">Organiziraj</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Personalize your Firefox just the way you like in Settings." approved="yes">
        <source>Personalize your Firefox just the way you like in Settings.</source>
        <target state="translated">V nastavitvah lahko prilagodite Firefox po svojih željah.</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Personalize your default search engine and more in Settings." approved="yes">
        <source>Personalize your default search engine and more in Settings.</source>
        <target state="translated">V nastavitvah lahko med drugim izberete tudi privzet iskalnik.</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Share" approved="yes">
        <source>Share</source>
        <target state="translated">Deli</target> <note>See http://mzl.la/1if9ODp</note>
      </trans-unit>
      <trans-unit id="Sign in to Firefox" approved="yes">
        <source>Sign in to Firefox</source>
        <target state="translated">Prijavite se v Firefox</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Start Browsing" approved="yes">
        <source>Start Browsing</source>
        <target state="translated">Začnite z brskanjem</target> <note>See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Sync Your Devices." approved="yes">
        <source>Sync Your Devices.</source>
        <target state="translated">Sinhronizirajte svoje naprave.</target> <note>ee http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Tap, hold and move the Firefox icon into your dock for easy access." approved="yes">
        <source>Tap, hold and move the Firefox icon into your dock for easy access.</source>
        <target state="translated">Za hiter dostop tapnite, zadržite in premaknite ikono Firefoxa v dok.</target> <note>See http://mzl.la/1if9ODp</note>
      </trans-unit>
      <trans-unit id="The Settings button is at the beginning of the Tabs Tray." approved="yes">
        <source>The Settings button is at the beginning of the Tabs Tray.</source>
        <target state="translated">Gumb za nastavitve je na začetku predala zavihkov.</target> <note>Accessibility label for an image. See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="The Show Tabs button is next to the Address and Search text field and displays the current number of open tabs." approved="yes">
        <source>The Show Tabs button is next to the Address and Search text field and displays the current number of open tabs.</source>
        <target state="translated">Gumb za prikaz zavihkov se nahaja ob polju za naslov in iskanje ter prikazuje trenutno število odprtih zavihkov.</target> <note>Accessibility label for an image. See http://mzl.la/1T8gxwo</note>
      </trans-unit>
      <trans-unit id="Use the share sheet to send links from other apps to Firefox." approved="yes">
        <source>Use the share sheet to send links from other apps to Firefox.</source>
        <target state="translated">Uporabite list za deljenje za pošiljanje povezav iz drugih aplikacij v Firefox.</target> <note>See http://mzl.la/1if9ODp</note>
      </trans-unit>
    </body>
  </file>
