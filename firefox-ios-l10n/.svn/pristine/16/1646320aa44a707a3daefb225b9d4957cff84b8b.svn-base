  <file original="Client/ClearPrivateData.strings" source-language="en" datatype="plaintext" target-language="bg">
    <header>
      <tool tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="7.0" build-num="7A220"/>
    </header>
    <body>
      <trans-unit id="Are you sure you want to clear all of your data? This will also close all open tabs." approved="yes">
        <source>Are you sure you want to clear all of your data? This will also close all open tabs.</source>
        <target state="translated">Сигурни ли сте, че искате да заличите всичките данни? Отворените подпрозорци ще бъдат затворени.</target> <note>Message shown in the dialog prompting users if they want to clear everything</note>
      </trans-unit>
      <trans-unit id="Browsing History" approved="yes">
        <source>Browsing History</source>
        <target state="translated">История на сърфирането</target> <note>Settings item for clearing browsing history</note>
      </trans-unit>
      <trans-unit id="Cache" approved="yes">
        <source>Cache</source>
        <target state="translated">Cache</target> <note>Settings item for clearing the cache</note>
      </trans-unit>
      <trans-unit id="Cancel" approved="yes">
        <source>Cancel</source>
        <target state="translated">Отказ</target> <note>Used as a button label in the dialog to cancel clear private data dialog</note>
      </trans-unit>
      <trans-unit id="Clear" approved="yes">
        <source>Clear</source>
        <target state="translated">Изчистване</target> <note>Used as a button label in the dialog to Clear private data dialog</note>
      </trans-unit>
      <trans-unit id="Clear Everything" approved="yes">
        <source>Clear Everything</source>
        <target state="translated">Изчистване всичко</target> <note>Title of the Clear private data dialog.</note>
      </trans-unit>
      <trans-unit id="Clear Private Data" approved="yes">
        <source>Clear Private Data</source>
        <target state="translated">Изчистване на лични данни</target> <note>Button in settings that clears private data for the selected items.
   Navigation title in settings.</note>
      </trans-unit>
      <trans-unit id="Cookies" approved="yes">
        <source>Cookies</source>
        <target state="translated">Cookies</target> <note>Settings item for clearing cookies</note>
      </trans-unit>
      <trans-unit id="Offline Website Data" approved="yes">
        <source>Offline Website Data</source>
        <target state="translated">Офлайн данни на страницa</target> <note>Settings item for clearing website data</note>
      </trans-unit>
      <trans-unit id="Saved Logins" approved="yes">
        <source>Saved Logins</source>
        <target state="translated">Запазени влизания</target> <note>Settings item for clearing passwords and login data</note>
      </trans-unit>
    </body>
  </file>
